{
  "name": "victorandrad/laravel-response-api",
  "description": "Adds JSON response to API in Laravel or Lumen projects",
  "keywords": [
    "laravel",
    "response",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Victor Andrade",
      "email": "victor.andrade62@hotmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "5.8.*",
    "laracasts/flash": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "VictorAndrad\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VictorAndrad\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~0.9"
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 src/",
    "fix-style": "phpcbf -p --standard=PSR2 src/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
